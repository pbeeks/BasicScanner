	<ContentPage.Content>
		<StackLayout>
			<Image Source="kiewitLogo.jpg" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" />
			<Entry Placeholder="username		" HorizontalOptions="CenterAndExpand" />
			<Entry Placeholder="password		" IsPassword="false" HorizontalOptions="CenterAndExpand" />
			<Button Text="Login" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" />
		</StackLayout>
	</ContentPage.Content>
</ContentPage>
